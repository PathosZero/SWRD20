    {
      "_id": "jqlnrt1olvrf28jf",
      "name": "Frightful Gaze",
      "type": "feat",
      "img": "systems/pf1/icons/skills/yellow_36.jpg",
      "data": {
        "description": {
          "value": "Creatures within 30 feet that meet a yeti’s gaze must succeed on a DC 13 Will save or stand paralyzed in fear for 1 round. This is a mind-affecting fear paralysis effect. A creature that successfully saves cannot be affected again by the frightful gaze of that yeti for 1 day. The save DC is Charisma-based."
        },
        "tags": [],
        "damage": {
          "parts": [],
          "critParts": [],
          "nonCritParts": []
        },
