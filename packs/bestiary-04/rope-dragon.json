          "dictionary": []
        },
        "scriptCalls": [],
        "learnedAt": {
          "class": [
            [
              "sorcerer/wizard",
              1
            ],
            [
              "bard",
              1
            ],
            [
              "psychic",
              1
            ],
            [
              "mesmerist",
