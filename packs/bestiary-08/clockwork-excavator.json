    {
      "_id": "8shf5r2rnf877l5q",
      "name": "Special Attack: Swallow Whole (2d6 Bludgeoning Damage, Ac 17, 11 Hp)",
      "type": "attack",
      "img": "systems/pf1/icons/skills/yellow_36.jpg",
