    {
      "_id": "gonem93fdd4dyiqf",
      "name": "Sense: Low-light Vision",
      "type": "feat",
      "img": "systems/pf1/icons/skills/shadow_11.jpg",
      "data": {
        "description": {
          "value": "A creature with low-light vision can see twice as far as a human in starlight, moonlight, torchlight, and similar conditions of dim light. It retains the ability to distinguish color and detail under these conditions."
        },
        "tags": [],
