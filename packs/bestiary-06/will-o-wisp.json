    {
      "_id": "0VM8AdwP25nHWnV8",
      "name": "Greater Feint",
      "type": "feat",
      "img": "systems/pf1/icons/feats/greater-feint.png",
      "data": {
        "description": {
          "value": "\n                <i>You are skilled at making foes overreact to your attacks.</i>\n                <br />\n                <br />\n                \n                <p><b>Prerequisites</b></p>\n                <p> Combat Expertise, Improved Feint, base attack bonus +6, Int 13.</p>\n                \n            <br />\n                <p><b>Benefits</b></p>\n                <p><p class=\"stat-block-1\"> Whenever you use feint to cause an opponent to lose his Dexterity bonus, he loses that bonus until the beginning of your next turn, in addition to losing his Dexterity bonus against your next attack.</p></p>\n                \n            <br />\n                <p><b>Normal</b></p>\n                <p><p class=\"stat-block-1\"> A creature you feint loses its Dexterity bonus against your next attack.</p></p>\n                \n            \n            "
